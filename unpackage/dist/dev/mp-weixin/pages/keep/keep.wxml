<view><view class="bg-white padding"><text class="text-black text-xl text-bold">晨间</text><view class="bg-white padding"><view class="grid col-5 grid-square"><block wx:for="{{testData}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><block wx:if="{{item.time==1}}"><view data-event-opts="{{[['longpress',[['clockIn',['$0','$1'],[[['testData','id',item.id,'id']],[['testData','id',item.id,'title']]]]]]]}}" class="bg-img" style="{{'background-image:'+('url(../../static/pic'+item.img+'.png)')+';'}}" bindlongpress="__e"></view></block></block></view></view><text class="text-black text-xl text-bold">午间</text><view class="bg-white padding"><view class="grid col-5 grid-square"><block wx:for="{{testData}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><block wx:if="{{item.time==2}}"><view data-event-opts="{{[['longpress',[['clockIn',['$0','$1'],[[['testData','id',item.id,'id']],[['testData','id',item.id,'title']]]]]]]}}" class="bg-img" style="{{'background-image:'+('url(../../static/pic'+item.img+'.png)')+';'}}" bindlongpress="__e"></view></block></block></view></view><text class="text-black text-xl text-bold">晚间</text><view class="bg-white padding"><view class="grid col-5 grid-square"><block wx:for="{{testData}}" wx:for-item="item" wx:for-index="__i2__" wx:key="id"><block wx:if="{{item.time==3}}"><view data-event-opts="{{[['longpress',[['clockIn',['$0','$1'],[[['testData','id',item.id,'id']],[['testData','id',item.id,'title']]]]]]]}}" class="bg-img" style="{{'background-image:'+('url(../../static/pic'+item.img+'.png)')+';'}}" bindlongpress="__e"></view></block></block></view></view></view><view class="{{['cu-modal',modalName=='DialogModal'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content text-red">打卡</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">{{''+note+''}}</view><view class="cu-bar bg-white col-2"><view class="padding"><button data-event-opts="{{[['tap',[['comfirm',['$event']]]]]}}" class="cu-btn bg-green " bindtap="__e">确认打卡</button></view><view class="padding"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn bg-red margin-left" bindtap="__e">放弃任务</button></view></view></view></view></view>